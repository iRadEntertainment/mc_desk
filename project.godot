; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="MC desktop app"
run/main_scene="res://scenes/main.tscn"
config/icon="res://icon.png"

[autoload]

netwrk="*res://scripts/netwrk.gd"
data_mng="*res://scripts/data_mng.gd"
glb="*res://scripts/glb.gd"
remote_func="*res://scripts/remote_func.gd"

[debug]

gdscript/warnings/enable=false

[editor_plugins]

enabled=PoolStringArray( "calendar_button" )

[locale]

translations=PoolStringArray( "res://localization/ciccio.en.translation", "res://localization/ciccio.it.translation", "res://localization/ciccio.da.translation", "res://localization/ciccio.pt.translation" )
locale_filter=[ 0, [ "da", "en", "it", "pt" ] ]

[rendering]

environment/default_environment="res://default_env.tres"
